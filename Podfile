source 'https://cdn.cocoapods.org/'
source 'https://github.com/divkit/divkit-ios'

use_frameworks!

project 'LayoutFrameworkBenchmark.xcodeproj'

platform :ios, '12.0'

inhibit_all_warnings!

target 'LayoutFrameworkBenchmark' do
    pod 'FlexLayout'
    pod 'NotAutoLayout'
    pod 'NKFrameLayoutKit'
    pod 'PinLayout'
    pod 'Texture'
    pod 'DivKit', :source => 'https://github.com/divkit/divkit-ios'
    pod 'VGSLBase', :source => 'https://github.com/yandex/vgsl'
    pod 'VGSLBaseTiny', :source => 'https://github.com/yandex/vgsl'
    pod 'VGSLBaseUI', :source => 'https://github.com/yandex/vgsl'
    pod 'VGSLCommonCore', :source => 'https://github.com/yandex/vgsl'
    pod 'VGSLNetworking', :source => 'https://github.com/yandex/vgsl'


    pod 'Reveal-SDK'
end
